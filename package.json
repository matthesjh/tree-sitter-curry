{
  "name": "tree-sitter-curry",
  "version": "0.0.3",
  "description": "Curry grammar for tree-sitter",
  "author": "Jan-Hendrik Matthes",
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "curry"
  ],
  "files": [
    "bindings/node/*",
    "prebuilds/**",
    "queries/*",
    "src/**",
    "binding.gyp",
    "CHANGELOG.md",
    "grammar.js",
    "*.wasm"
  ],
  "homepage": "https://github.com/matthesjh/tree-sitter-curry",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthesjh/tree-sitter-curry.git"
  },
  "bugs": {
    "url": "https://github.com/matthesjh/tree-sitter-curry/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "node-addon-api": "^8.1.0",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js && npx tree-sitter test",
    "lint": "eslint *.js"
  }
}
