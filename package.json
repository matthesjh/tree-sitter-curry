{
  "name": "tree-sitter-curry",
  "version": "0.0.2",
  "description": "Curry grammar for tree-sitter",
  "author": "Jan-Hendrik Matthes",
  "license": "MIT",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "curry"
  ],
  "files": [
    "binding.gyp",
    "grammar.js",
    "CHANGELOG.md",
    "bindings/node/*",
    "prebuilds/**",
    "queries/*",
    "src/**"
  ],
  "homepage": "https://github.com/matthesjh/tree-sitter-curry",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthesjh/tree-sitter-curry.git"
  },
  "bugs": {
    "url": "https://github.com/matthesjh/tree-sitter-curry/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "node-addon-api": "^8.1.0",
    "node-gyp-build": "^4.8.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.22.6"
  },
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "build": "tree-sitter generate --no-bindings",
    "build-wasm": "tree-sitter build --wasm",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse",
    "lint": "eslint *.js"
  },
  "tree-sitter": [
    {
      "scope": "source.curry",
      "file-types": [
        "curry"
      ],
      "first-line-regex": "#!.*\\bruncurry$",
      "injection-regex": "^curry$"
    }
  ]
}
