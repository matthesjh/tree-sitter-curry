{
  "name": "tree-sitter-curry",
  "version": "0.0.3",
  "description": "Curry grammar for tree-sitter",
  "author": "Jan-Hendrik Matthes",
  "license": "MIT",
  "type": "module",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "curry"
  ],
  "files": [
    "bindings/node/*",
    "prebuilds/**",
    "queries/*",
    "src/**",
    "binding.gyp",
    "CHANGELOG.md",
    "grammar.js",
    "tree-sitter.json",
    "*.wasm"
  ],
  "homepage": "https://github.com/matthesjh/tree-sitter-curry",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthesjh/tree-sitter-curry.git"
  },
  "bugs": {
    "url": "https://github.com/matthesjh/tree-sitter-curry/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.0",
    "eslint": "^9.39.2",
    "prebuildify": "^6.0.1",
    "prettier": "^3.8.0",
    "tree-sitter": "^0.25.0",
    "tree-sitter-cli": "^0.26.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js && tree-sitter test",
    "setup": "npm rebuild tree-sitter tree-sitter-cli --ignore-scripts false && node-gyp-build",
    "generate": "tree-sitter generate",
    "lint:check": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --log-level warn --check .",
    "format:write": "prettier --log-level silent --write ."
  }
}
