{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "curry",
      "camelcase": "Curry",
      "title": "Curry",
      "scope": "source.curry",
      "file-types": ["curry"],
      "injection-regex": "^curry$",
      "first-line-regex": "#!.*\\bruncurry$",
      "class-name": "TreeSitterCurry"
    }
  ],
  "metadata": {
    "version": "0.0.3",
    "license": "MIT",
    "description": "Curry grammar for tree-sitter",
    "authors": [
      {
        "name": "Jan-Hendrik Matthes"
      }
    ],
    "links": {
      "repository": "https://github.com/matthesjh/tree-sitter-curry.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
